{
    "rules": {
        "users": {
            "$uid": {
                ".read": "$uid === auth.uid",
                ".write": "$uid === auth.uid"
            }
        },
        "sessions": {
            "$sessionId": {
                ".read": "auth != null",
                ".write": "auth != null"
            }
        },
        "presence": {
            ".read": "auth != null",
            "$uid": {
                ".read": "auth != null",
                ".write": "$uid === auth.uid"
            }
        },
        "typing": {
            "$roomId": {
                ".read": "auth != null",
                "$uid": {
                    ".write": "$uid === auth.uid"
                }
            }
        },
        ".read": false,
        ".write": false
    }
}